
CREATE TABLE Cliente
(id int,
nome varchar(100),
endereco varchar(100),
cidade varchar(100),
estado char(2),
genero varchar(20);
email varchar(100));


CREATE TABLE Produto
(id int,
codBarras int,
descricao varchar(100),
genero varchar(30),
unidade varchar(30),
custo bigint,
margemLucro bigint);


CREATE TABLE Usuario
(idCliente int,
idUs int,
senha varchar(100));


create table Venda(
nNota int not null,
cliente varchar(100),
produto varchar(100),
qtd int,
dataHora varchar(50));




